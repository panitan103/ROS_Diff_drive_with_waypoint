    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 0.13 -0.0425" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://bogie_description/meshes/right_wheel.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="left_wheel">
  <inertial>
    <origin xyz="2.6999999016951607e-08 0.004999994999997953 2.0816681711721685e-17" rpy="0 0 0"/>
    <mass value="0.44544838584790525"/>
    <inertia ixx="0.000205" iyy="0.000402" izz="0.000205" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.13 -0.0425" rpy="0 0 0"/>
