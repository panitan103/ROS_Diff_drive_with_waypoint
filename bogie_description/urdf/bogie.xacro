    <geometry>
      <mesh filename="package://bogie_description/meshes/left_wheel.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="imu_link">
  <inertial>
    <origin xyz="0.0 0.0 -0.0025000000000000022" rpy="0 0 0"/>
    <mass value="0.0157"/>
    <inertia ixx="1e-06" iyy="1e-06" izz="1e-06" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.0 -0.2975" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://bogie_description/meshes/imu_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 -0.0 -0.2975" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://bogie_description/meshes/imu_link.stl" scale="0.001 0.001 0.001"/>
