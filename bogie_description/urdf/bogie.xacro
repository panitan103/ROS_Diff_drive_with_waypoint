  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://bogie_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://bogie_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="right_wheel">
  <inertial>
    <origin xyz="1.0017341003671332e-17 -0.0050000000000000044 6.938893903907228e-18" rpy="0 0 0"/>
    <mass value="0.44544838584790514"/>
    <inertia ixx="0.000205" iyy="0.000402" izz="0.000205" ixy="-0.0" iyz="-0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 0.13 -0.0425" rpy="0 0 0"/>
