<link name="base_laser_link">
  <inertial>
    <origin xyz="0.0 0.0 0.009750000000000036" rpy="0 0 0"/>
    <mass value="0.5891016369241163"/>
    <inertia ixx="0.000199" iyy="0.000199" izz="0.000361" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.0 -0.344" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://bogie_description/meshes/base_laser_link.stl" scale="0.001 0.001 0.001"/>
